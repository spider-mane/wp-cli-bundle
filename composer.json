{
  "name": "webtheory/wp-cli-bundle",
  "description": "WP-CLI bundle for exclusive use as a Composer dependency",
  "keywords": [
    "cli",
    "wordpress"
  ],
  "homepage": "https://wp-cli.org",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/wp-cli/wp-cli-bundle/issues",
    "source": "https://github.com/wp-cli/wp-cli-bundle",
    "docs": "https://make.wordpress.org/cli/handbook/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "wp-cli/cache-command": "^2.0",
    "wp-cli/checksum-command": "^2.1",
    "wp-cli/config-command": "^2.1",
    "wp-cli/core-command": "^2.1",
    "wp-cli/cron-command": "^2.0",
    "wp-cli/db-command": "^2.0",
    "wp-cli/embed-command": "^2.0",
    "wp-cli/entity-command": "^2.0",
    "wp-cli/eval-command": "^2.0",
    "wp-cli/export-command": "^2.0",
    "wp-cli/extension-command": "^2.1",
    "wp-cli/i18n-command": "^2.0",
    "wp-cli/import-command": "^2.0",
    "wp-cli/language-command": "^2.0",
    "wp-cli/maintenance-mode-command": "^2.0",
    "wp-cli/media-command": "^2.0",
    "wp-cli/package-command": "^2.1",
    "wp-cli/rewrite-command": "^2.0",
    "wp-cli/role-command": "^2.0",
    "wp-cli/scaffold-command": "^2.0",
    "wp-cli/search-replace-command": "^2.0",
    "wp-cli/server-command": "^2.0",
    "wp-cli/shell-command": "^2.0",
    "wp-cli/super-admin-command": "^2.0",
    "wp-cli/widget-command": "^2.0",
    "wp-cli/wp-cli": "^2.8"
  },
  "suggest": {
    "psy/psysh": "Enhanced `wp shell` functionality"
  },
  "config": {
    "sort-packages": true
  }
}
